{
  "command": {
    "create": {
      "license": "MIT"
    },
    "publish": {
      "allowBranch": "release/*",
      "bump": "patch",
      "conventionalCommits": true,
      "changelog": false,
      "message": "chore(release): Publish"
    }
  },
  "packages": ["packages/*"],
  "loglevel": "success",
  "version": "independent",
  "npmClient": "yarn",
  "registry": "https://registry.npmjs.org/",
  "ignoreChanges": [
    "CHANGELOG.md",
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/__testfixtures__/**"
  ],
  "$schema": "node_modules/lerna/schemas/lerna-schema.json"
}
