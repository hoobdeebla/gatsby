{
  "name": "gatsby-codemods",
  "version": "4.17.0-next.0",
  "description": "A collection of codemod scripts for use with JSCodeshift that help migrate to newer versions of Gatsby.",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"  --extensions \".ts,.tsx,.js\"",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\"  --extensions \".ts,.tsx,.js\"",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-codemods#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-codemods"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-syntax-jsx": "^7.28.6",
    "@babel/plugin-syntax-typescript": "^7.28.6",
    "@babel/plugin-transform-class-properties": "^7.28.6",
    "@babel/runtime": "^7.28.6",
    "execa": "^8.0.1",
    "graphql": "^16.13.0",
    "jscodeshift": "^17.3.0",
    "recast": "^0.23.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@types/jscodeshift": "^17.3.0",
    "babel-preset-gatsby-package": "^3.17.0-next.0",
    "cross-env": "^10.1.0"
  },
  "engines": {
    "node": ">=18.0.0 <26"
  },
  "bin": "./bin/gatsby-codemods.js"
}
