{
  "name": "gatsby-plugin-mdx",
  "version": "5.17.0-next.0",
  "description": "MDX integration for Gatsby",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-plugin-mdx#readme",
  "scripts": {
    "build": "babel src --out-dir dist/ --ignore \"**/__tests__\" --ignore \"**/__mocks__\" --extensions \".ts\"",
    "typegen": "tsc --emitDeclarationOnly --declaration --declarationDir dist/",
    "prepare": "cross-env NODE_ENV=production npm run clean && npm run build && npm run typegen",
    "watch": "babel -w src --out-dir dist/ --ignore \"**/__tests__\" --extensions \".ts,.tsx\"",
    "test": "jest",
    "clean": "del-cli dist/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-plugin-mdx"
  },
  "peerDependencies": {
    "@mdx-js/react": "^3.0.0",
    "gatsby": "^5.0.0-next",
    "gatsby-source-filesystem": "^5.0.0-next",
    "react": "^18.0.0 || ^19.0.0 || ^0.0.0",
    "react-dom": "^18.0.0 || ^19.0.0 || ^0.0.0"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "acorn": "^8.16.0",
    "acorn-jsx": "^5.3.2",
    "astring": "^1.9.0",
    "deepmerge": "^4.3.1",
    "estree-util-build-jsx": "^3.0.1",
    "fs-extra": "^11.3.3",
    "gatsby-core-utils": "^4.17.0-next.0",
    "gatsby-plugin-utils": "^4.17.0-next.0",
    "gray-matter": "^4.0.3",
    "mdast-util-mdx": "^3.0.0",
    "mdast-util-to-hast": "^13.2.1",
    "mdast-util-toc": "^7.1.0",
    "rehype-infer-description-meta": "^2.0.0",
    "rehype-unwrap-images": "^1.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.1.0",
    "vfile": "^6.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@types/estree": "^1.0.8",
    "@types/fs-extra": "^11.0.4",
    "@types/mdast": "^4.0.4",
    "@types/unist": "^3.0.3",
    "babel-preset-gatsby-package": "^3.17.0-next.0",
    "cross-env": "^10.1.0",
    "del-cli": "^7.0.0",
    "opentracing": "^0.14.7",
    "typescript": "^5.9.3",
    "webpack": "^5.105.3"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "mdx",
    "markdown",
    "remark",
    "rehype"
  ],
  "files": [
    "dist/*",
    "gatsby-node.js"
  ]
}
