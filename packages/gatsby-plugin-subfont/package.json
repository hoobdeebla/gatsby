{
  "name": "gatsby-plugin-subfont",
  "version": "5.16.0-next.0",
  "description": "Runs the font delivery optimizing CLI tool subfont on the homepage of your site during the Gatsby build",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-plugin-subfont#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-plugin-subfont"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.28.3",
    "subfont": "^7.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "babel-preset-gatsby-package": "^3.16.0-next.0",
    "cross-env": "^10.0.0"
  },
  "peerDependencies": {
    "gatsby": "^5.0.0-next"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
