    "axios": "^1.11.0",
    "chalk": "^4.1.2",
    "cheerio": "1.0.0-rc.12",
    "semver": "^7.7.2",
    "sharp": "^0.32.6",
    "unist-util-select": "^3.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "babel-preset-gatsby-package": "^3.16.0-next.0",
    "cross-env": "^10.0.0"
  },
  "keywords": [
