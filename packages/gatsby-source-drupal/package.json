    "fastq": "^1.20.1",
    "gatsby-plugin-utils": "^4.17.0-next.0",
    "gatsby-source-filesystem": "^5.17.0-next.0",
    "got": "^14.6.6",
    "http2-wrapper": "^2.2.1",
