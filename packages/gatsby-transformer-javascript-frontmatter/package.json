{
  "name": "gatsby-transformer-javascript-frontmatter",
  "description": "Gatsby transformer plugin for JavaScript to extract exports.frontmatter statically.",
  "version": "5.17.0-next.0",
  "author": "Jacob Bolda <me@jacobbolda.com>",
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-transformer-javascript-frontmatter#readme",
  "dependencies": {
    "@babel/parser": "^7.29.0",
    "@babel/runtime": "^7.28.6",
    "@babel/traverse": "^7.29.0",
    "lodash": "^4.17.23"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "babel-preset-gatsby-package": "^3.17.0-next.0",
    "cross-env": "^10.1.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "js"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^5.0.0-next",
    "gatsby-source-filesystem": "^5.0.0-next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-transformer-javascript-frontmatter"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
  },
  "engines": {
    "node": ">=18.0.0 <26"
  }
